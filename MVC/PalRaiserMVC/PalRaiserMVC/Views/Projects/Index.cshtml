@using Microsoft.AspNetCore.Http;
@inject IHttpContextAccessor HttpContextAccessor

<h2 class="text-info">Session Id: @HttpContextAccessor.HttpContext.Session.GetInt32("currentUser")</h2>
<br />

<br />
<div class="container row p-0 m-0">
    <div class="col-6">

    </div>
    <div class="col-3 offset-3">
        <a asp-action="UpsertProj" asp-controller="Projects" class="btn btn-info form-control text-white">
            Add New Project
        </a>
    </div>
    <div class="col-12 border p-3">
        <table id="DT_load" class="table table-striped table-bordered" style="width:100%">
            <thead>
                <tr>
                    <th>Name</th>
                    <th>Type</th>
                    <th>Raised</th>
                    <th>Likes</th>
                    <th>Dislikes</th>
                    <th>Reports</th>
                    <th></th>
                </tr>
            </thead>
        </table>
    </div>
</div>

@section Scripts {
    <script src="~/js/finalProjectsListVer2.js"></script>
}
